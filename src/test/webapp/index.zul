<vbox hflex="1">
    <label value="Example 1"/>
    <style>
        .github-link {
        color: #000;
        }
        .github-link:hover {
        color: #4183C4;
        }
    </style>
    <a sclass="github-link"
       href="http://fortawesome.github.io/Font-Awesome/icon/github/">
        <icon type="icon-github" style="text-decoration:underline"/>
    </a>
    <label value="Example 2"/>
    <a href="http://fortawesome.github.io/Font-Awesome/">
        <icon type="icon-double-angle-right"/>
        <label value="Goto Font Awesome"/>
    </a>
    <label value="Example 3"/>
    <icon id="i" type="icon-hand-right">
        <attribute name="onClick">
            i.setType("icon-thumbs-up");
        </attribute>
    </icon>
    <label value="Example 4"/>
    <div xmlns:c="client">
        <icon type="icon-hand-right"
              c:onClick="this.setType('icon-thumbs-up')"/>
    </div>
